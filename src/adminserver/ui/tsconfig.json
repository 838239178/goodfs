{
  "compilerOptions": {
    "baseUrl": "./",
    "allowJs": true,
    "paths": {
      "@/*": ["src/*"]
    },
    "types": ["@intlify/vite-plugin-vue-i18n/client"]
  },
  "exclude": ["node_modules", "dist"],
  "include": [
    "typed-router.d.ts",
    "components.d.ts",
    "auto-imports.d.ts",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.vue"
  ],
  "extends": "@vue/tsconfig/tsconfig.json"
}
